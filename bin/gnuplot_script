set datafile separator ","
set terminal png size 900,400
set title "Cannonball Trajectory"
set ylabel "Y (Kilometers)"
set xlabel "X (Kilometers)"
set ylabel textcolor rgb "#4E78A7"
set key left top
set grid
set output 'output/cannonball.png'
set ytics 0,3,15 nomirror tc lt 1 textcolor rgb "#4E78A7"
set autoscale  y
unset border
plot "output/cnl.csv" using ($1/1000):($2/1000) title 'CNL FP' with lines linecolor "#A1CBE8"


